<Window x:Class="SystembolagetWebScraper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SystembolagetWebScraper"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1500">
    <Grid>
        <ListBox ItemsSource="{Binding Products}" HorizontalAlignment="Left" VerticalAlignment="Top" Width="750" Height="800">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="{Binding Name}" Margin="5" Width="130"/>
                        <TextBlock Text="{Binding Country}" Margin="5" Width="100"/>
                        <TextBlock Text="{Binding Volume}" Margin="5" Width="100"/>
                        <TextBlock Text="{Binding Alcohol}" Margin="5" Width="100"/>
                        <TextBlock Text="{Binding Price}" Margin="5" Width="100"/>
                        <TextBlock Text="{Binding APK}" Margin="5" Width="100"/>
                    </StackPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
    </Grid>
</Window>
